
@{
    ViewBag.Title = "About";
    Layout = "~/Views/Shared/_PublicLayout.cshtml";
}


<main>
    <!-- Page title -->
    <section class="relative pt-24 lg:pb-96">
        <picture class="pointer-events-none absolute inset-0 -z-10 dark:hidden">
            <img src="~/Content/publicAssets/img/gradient_light.jpg" alt="gradient" class="h-full w-full" />
        </picture>
        <div class="container">
            <!-- Page Title -->
            <div class="mx-auto max-w-2xl py-16 text-center">
                <h1 class="mb-8 font-display text-4xl font-medium text-jacarta-700 dark:text-white">About Trading Manitou</h1>
                <p class="text-lg leading-normal dark:text-jacarta-300">
                    Find out more about us and Trading Manitou.
                </p>
            </div>
        </div>
    </section>

    <!-- Intro / Statistics -->
    <section class="pb-24">
        <div class="container">
            <!-- Video Lightbox -->
            <figure class="relative mt-16 overflow-hidden rounded-3xl before:absolute before:inset-0 before:bg-jacarta-900/25 lg:-mt-96">
                @*<img src="~/Content/publicAssets/img/about/video_cover.jpg" class="w-full" alt="video" />*@
                <img src="~/Content/publicAssets/img/trailer-jpg.jpg" class="w-full" alt="video" />
                <a href="https://www.youtube.com/embed/fhCM-fDgpec"
                   data-bs-toggle="modal"
                   data-bs-target=".video-lightbox"
                   class="js-video-modal-trigger absolute top-1/2 left-1/2 flex h-24 w-24 -translate-y-1/2 -translate-x-1/2 items-center justify-center rounded-full border-2 border-white transition-transform will-change-transform hover:scale-90">
                    <svg xmlns="http://www.w3.org/2000/svg"
                         viewBox="0 0 24 24"
                         width="24"
                         height="24"
                         class="h-8 w-8 fill-white">
                        <path fill="none" d="M0 0h24v24H0z" />
                        <path d="M19.376 12.416L8.777 19.482A.5.5 0 0 1 8 19.066V4.934a.5.5 0 0 1 .777-.416l10.599 7.066a.5.5 0 0 1 0 .832z" />
                    </svg>
                </a>
            </figure>
        </div>
    </section>
    <!-- end intro / statistics -->
    <!-- Story -->
    <section class="relative py-24 dark:bg-jacarta-800">
        <picture class="pointer-events-none absolute inset-0 -z-10 dark:hidden">
            <img src="~/Content/publicAssets/img/gradient_light.jpg" alt="gradient" class="h-full w-full" />
        </picture>

        <div class="container">
            <div class="lg:flex lg:justify-between">
                <!-- Image -->
                <div class="lg:w-[55%]">
                    <div class="relative">
                        <svg viewbox="0 0 200 200"
                             xmlns="http://www.w3.org/2000/svg"
                             class="mx-auto mt-8 w-[100%] ">
                            <defs>
                                <clipPath id="clipping" clipPathUnits="userSpaceOnUse">
                                    <path d="
                    M 0, 100
                    C 0, 17.000000000000004 17.000000000000004, 0 100, 0
                    S 200, 17.000000000000004 200, 100
                        183, 200 100, 200
                        0, 183 0, 100
                "
                                          fill="#9446ED"></path>
                                </clipPath>
                            </defs>
                            <g clip-path="url(#clipping)">
                                <!-- Bg image -->
                                <image href="~/Content/publicAssets/img/about/about-img.png" width="200" height="200" clip-path="url(#clipping)" />
                            </g>
                        </svg>
                        <img src="~/Content/publicAssets/img/hero/3D_elements.png" alt="" class="absolute top-0 animate-fly" />
                    </div>
                </div>

                <!-- Info -->
                <div class="py-20 lg:w-[45%] lg:pl-16">
                    <h2 class="mb-6 font-display text-2xl text-jacarta-700 dark:text-white">
                        Trading Manitou’s mission
                    </h2>
                    <p class="mb-8 text-lg leading-normal dark:text-jacarta-300">
                        We designed Trading Manitou with the idea at heart to make automated crypto trading accessible to everyone and provide opportunities to traders of all experience levels to make a profit in the ever-growing crypto market.

                    </p>
                    <p class="mb-10 dark:text-jacarta-300">
                        Trading Manitou’s distinguishable feature is an easy-to-use, yet powerful interface that provides everything you need to automate your trading, and we will always keep it this way. The Trading Manitou team works hard to deliver new features monthly, yet keep the service understandable for all levels of traders.
                    </p>
                    <div class="flex space-x-4 sm:space-x-10">
                        <div class="flex space-x-4">
                            <svg xmlns="http://www.w3.org/2000/svg"
                                 viewBox="0 0 24 24"
                                 width="24"
                                 height="24"
                                 class="h-8 w-8 shrink-0 fill-accent">
